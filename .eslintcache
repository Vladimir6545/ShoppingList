)\r\n\r\n  React.useEffect(() => {\r\n    localStorage.setItem('shoppingList', JSON.stringify(massive))\r\n  })\r\n\r\n  function ToggleItem(id) {\r\n    setMassive(\r\n      massive.map(items => {\r\n        if (items.id === id) {\r\n          items.complete = !items.complete\r\n        }\r\n        return items\r\n      })\r\n    )\r\n  }\r\n\r\n  function RemoveItemFromList(id) {\r\n    setMassive(massive.filter(item => item.id !== id))\r\n  }\r\n\r\n  function ClearLocalStorage() {\r\n    if (localStorage.getItem('shoppingList') !== null) {\r\n      localStorage.clear('shoppingList')\r\n      const temp = localStorage.getItem('shoppingList')\r\n      setMassive(JSON.parse(temp))\r\n      HowManyCheckItems()\r\n    }\r\n  }\r\n\r\n  function ClearLocalStorageWithCheckItems() {\r\n    if (localStorage.getItem('shoppingList') !== null) {\r\n      const temp = localStorage.getItem('shoppingList')\r\n      let newList = JSON.parse(temp)\r\n      newList = newList.filter(item => item.complete === false)\r\n      setMassive(newList)\r\n    }\r\n  }\r\n\r\n  function HowManyCheckItems() {\r\n    let checkboxes = document.querySelectorAll('input[type=\"checkbox\"]:checked')\r\n    return checkboxes.length\r\n  }\r\n\r\n\r\n  function AddItemList(title) {\r\n    if (massive === null) {\r\n      setMassive(massive = [])\r\n    }\r\n    setMassive(massive.concat([\r\n      {\r\n        title,\r\n        id: Date.now(),\r\n        complete: false\r\n      }\r\n    ]))\r\n  }\r\n\r\n  return (\r\n    <Context.Provider value={{ RemoveItemFromList }}>\r\n      <div className='wrapper'>\r\n        <h1 style={{ textAlign: 'center' }}>Shopping List</h1>\r\n        <AddItem onCreate={ AddItemList } />\r\n        <br/>\r\n        <div style={{overflow: 'hidden'}}>\r\n         \r\n         <p  style={{float: 'left'}} style={{paddingLeft: '3rem'}}> { HowManyCheckItems() } items selected\r\n            { \r\n              massive !== null\r\n              && massive.length > 1\r\n              && massive.some(item => item.complete === true)\r\n              && <button className='btnDeleteCheckAll' onClick={() => ClearLocalStorageWithCheckItems() }>Delete selected</button>\r\n            }\r\n           \r\n           \r\n            {\r\n              massive !== null\r\n              && massive.length > 1\r\n              && <button className='btnDeleteAll' onClick={() => ClearLocalStorage() }>Delete all</button>\r\n            }\r\n          </p>\r\n        </div>\r\n        {\r\n          massive !== null && massive.length ?\r\n            <CurrentList massive={massive} onToggle={ToggleItem}/> :\r\n            <h2>Empty shopping list</h2>\r\n        }\r\n      </div>\r\n    </Context.Provider>\r\n  )\r\n}\r\n\r\nexport default App;\r\n","e:\\Projects\\listbuyreact\\src\\Components\\List.js",[],"e:\\Projects\\listbuyreact\\src\\Components\\ListItems.js",[],["61","62"],"e:\\Projects\\listbuyreact\\src\\context.js",[],"e:\\Projects\\listbuyreact\\src\\Components\\AddItemToList.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":2,"message":"68","line":25,"column":13,"nodeType":"69","endLine":25,"endColumn":22},{"ruleId":"63","replacedBy":"70"},{"ruleId":"65","replacedBy":"71"},{"ruleId":"72","severity":1,"message":"73","line":80,"column":38,"nodeType":"74","endLine":80,"endColumn":67},{"ruleId":"63","replacedBy":"75"},{"ruleId":"65","replacedBy":"76"},"no-native-reassign",["77"],"no-negated-in-lhs",["78"],"react/no-typos","Typo in static class property declaration","Identifier",["77"],["78"],"react/jsx-no-duplicate-props","No duplicate props allowed","JSXAttribute",["77"],["78"],"no-global-assign","no-unsafe-negation"]